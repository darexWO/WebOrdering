    margin:0 8px 0 8px;
    cursor: pointer;
}

.product_item .dezen {
    height:30px;
    line-height:30px;
    font-size: 15px;
    font-weight:600;
    color:#4c4b4b;
}

.product_item .cena {
    height:20px;
    line-height:20px;
    font-size: 14px;
    font-weight:600;
    color:#e47818;
}

.product_item .proizvodjac {
    height:15px;
    line-height:15px;
    font-size: 11px;
