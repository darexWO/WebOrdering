}
.product_item .zalihe {
    height:20px;
    line-height:20px;
    font-size: 9px;
    font-weight:600;
    color:#e37217;
    padding:0 5px 0 5px;
    background-color:#F1F1F1;
}

.product_item .kolicina {
    height:30px;
