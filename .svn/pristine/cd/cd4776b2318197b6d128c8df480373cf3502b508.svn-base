/* img buttons */
.btn, .btn:visited {
    border-width: 1px;
    border-style: solid;
    border-color: #b3b3b3 #070707 #070707 #b3b3b3;
    border-radius: 3px;
    text-align: center;
    padding: 3px 4px;
    cursor:pointer;
    overflow: visible;
    background-color: #585858;
}

.btn:hover {
    text-decoration: none;
    color:#e26f16;
    background-color:#eeb428;
}

.btn:active {
    border-color: #070707 #b3b3b3 #b3b3b3 #070707;
}

.btn.search, .btn.login {
    padding: 4px 5px;
    width:75px;
    font-size:9px;
    color:#ffffff;
}
.btn.search:hover, .btn.login:hover {
    color:#ffffff;
}

.btn.basket, .btn.wait, .btn.potvrdi, .btn.ponisti {
    background-color: #e26f16;
    padding: 3px 25px 3px 5px;
    color:#ffffff;
}
.btn.basket:hover, .btn.potvrdi:hover, .btn.ponisti:hover, .btn.wait:hover {
    background-color:#eeb428;
    padding: 3px 25px 3px 5px;
}

.btn.basket {
    background: url(../images/basket.png) no-repeat center right;
    background-color: #e26f16;
    background-position: 97% 50%;
    margin:0 5px 0 0;
}

.btn.zoom {
    background: url(../images/zoom.png) no-repeat center right;
    background-color:#b2b2b2;
    background-position: 95% 50%;
    padding: 3px 20px 3px 5px;
    margin:0 0 0 5px;
    color:#ffffff;
}

.btn.wait {
    background: url(../images/wait16.gif) no-repeat center right;
    background-color: #e26f16;
    background-position: 95% 50%;
    margin:0 5px 0 0;
}

.btn.potvrdi {
    background: url(../images/potvrdi.png) no-repeat center right;
    background-color: #4c4b4b;
    background-position: 95% 50%;
    font-size:14px;
}
.btn.ponisti {
    background: url(../images/ponisti.png) no-repeat center right;
    background-color: #4c4b4b;
    background-position: 95% 50%;
    font-size:12px;
}
/* end img buttons */

/* prikaz proizvoda */
.product_item {
    position:relative;
    width:230px;
    height:352px;
    border-radius:5px;
