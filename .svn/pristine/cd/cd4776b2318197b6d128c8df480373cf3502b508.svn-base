a {
    text-decoration: none;
}

a:hover {
    text-decoration: underline;
}


a img {
    border: none;
}

a, a:active, a:focus {
    outline: none;
}


h3 {
    font-size: 12px;
}
h4 {
    font-size: 11px;
}
h5 {
    font-size: 10px;
}

p, ul, ol, dl {
    margin-bottom: 15px;
}


p img, span img, table img {
    display: inline;
}


ul {
    padding: 0;
    margin: 0;
    list-style: none;
}

.selected {
    color: #e26f16;
}



select, input {
    font-family: verdana, arial, tahoma;
    font-size: 10px;
    font-weight: 600;
    text-align: center;
    border-width:1px;
    color: #4c4b4b;
}
input[type=text] {
    border:none;
    width:150px;
    margin:0;
    height:20px;
    line-height: 20px;
    border-radius:3px;
}
input[type=password] {
    border:none;
    width:150px;
    margin:0;
    height:20px;
    line-height: 20px;
    border-radius:3px;
}


select {
    -webkit-appearance: button;
    -webkit-border-radius: 2px;
    -webkit-box-shadow: 0px 1px 3px rgba(0, 0, 0, 0.1);
    -webkit-padding-end: 20px;
    -webkit-padding-start: 2px;
    -webkit-user-select: none;
    background: #fff url('../images/down-arrow.gif') no-repeat center right;
    margin: 0;
    /*******overflow: hidden;******/
    padding-top: 2px;
    padding-bottom: 2px;
    white-space: nowrap;
    border: none;
    height:20px;
}

.ellipsis {
    white-space: nowrap;
    overflow: hidden;
}

.multiline {
    white-space: normal;
    overflow: hidden;
}

.dark_gray_gradient {
    background: #565656; /* Old browsers */
    /* SVG as background image (IE9/Chrome/Safari/Opera) */
    background:url(data:image/svg+xml;base64,);

    background: -moz-linear-gradient(top,  #756e75 0%, #141413 100%); /* FF3.6+ */
    background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#756e75), color-stop(100%,#141413)); /* Chrome,Safari4+ */
    background: -webkit-linear-gradient(top,  #756e75 0%,#141413 100%); /* Chrome10+,Safari5.1+ */
    background: -o-linear-gradient(top,  #756e75 0%,#141413 100%); /* Opera 11.10+ */
    background: -ms-linear-gradient(top,  #756e75 0%,#141413 100%); /* IE10+ */
    background: linear-gradient(to bottom,  #756e75 0%,#141413 100%); /* W3C */
    filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#756e75', endColorstr='#141413',GradientType=0 ); /* IE6-9 */
    zoom:1
}

.light_gray_gradient {
    background: #dcdcdc; /* Old browsers */
    /* SVG as background image (IE9/Chrome/Safari/Opera) */
    background: url(data:image/svg+xml;base64,);

    background: -moz-linear-gradient(top,  #ffffff 0%, #666666 47%, #dcdcdc 100%); /* FF3.6+ */
    background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#ffffff), color-stop(47%,#666666), color-stop(100%,#dcdcdc)); /* Chrome,Safari4+ */
    background: -webkit-linear-gradient(top,  #ffffff 0%,#fefefe 47%,#dcdcdc 100%); /* Chrome10+,Safari5.1+ */
    background: -o-linear-gradient(top,  #ffffff 0%,#fefefe 47%,#dcdcdc 100%); /* Opera 11.10+ */
    background: -ms-linear-gradient(top,  #ffffff 0%,#fefefe 47%,#dcdcdc 100%); /* IE10+ */
    background: linear-gradient(to bottom,  #ffffff 0%,#fefefe 47%,#dcdcdc 100%); /* W3C */
    filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#fefefe', endColorstr='#dcdcdc',GradientType=0 ); /* IE6-9 */
    zoom:1
}


/* header - log i dva horizontalna menija G2 i G3 */
#header {
    min-width:1140px;
    background-color: #e3e3e3;
    background-image: -moz-linear-gradient(center top, #f3f3f3, #e3e3e3);
    background-image: -webkit-gradient(linear, left top, left bottom, color-stop(0, #f3f3f3), color-stop(1, #e3e3e3));
    background-image: linear-gradient(top, #f3f3f3, #e3e3e3);
    filter: progid:DXImageTransform.Microsoft.gradient(startColorStr = '#f3f3f3', EndColorStr = '#e3e3e3');
    background-repeat: no-repeat;
    height: 100%;
    /* change the box model to exclude the padding from the calculation of 100% height (IE8+) */
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
}

.G1 {
    width: 1100px;
    padding: 20px;
    padding-bottom: 10px;
    margin: 0px auto;
}

.G2 {
    width:1100px;
    margin-top: 10px;
    background: transparent url('../images/menu-d.png') repeat-x left top;
    border-radius: 5px;
    height: 37px;

}

.G2 ul li {
    display: block;
    float: left;
    padding: 0px 15px 0px 15px;
    margin: 8px 0px 8px 0px;
    border-left: 2px solid white;
}

.G2 ul li:first-child {
    border-left: none
}

.G2 ul li a {
    color: #ffffff;
    font-size: 12px;
    font-weight:600;
    line-height: 20px;
}

.G2 ul li a.selected {
    color:#f2af26;
}

.G2 ul li a:hover, .G2 ul li a.active {
    color:#f2af26;
    text-decoration: none;
}

.G3 {
    margin-top: 20px;
    height: 37px;
    color:#4C4C4C;
}

.G3 ul li {
    display: block;
    float: left;
    padding: 0px 15px 0px 15px;
    margin: 8px 0px 8px 0px;
    border-left: 2px solid #4c4b4b;
}

.G3 ul li:first-child {
    border-left: none
}

.G3 ul li a {
    color: #4c4b4b;
    font-size:11px;
    font-weight:600;
    line-height: 19px;
}

.G3 ul li a:hover, .G3 ul li a.active, .G3 ul li a.selected {
    color:#e26f16;
    text-decoration: none;
}


.G3_L {
    height: 35px;
    float: left;
    border-radius: 5px 0 0 5px;
    border: solid 1px #b3b3b3;
    border-right:0;
}

.G3_D {
    text-align: right;
    background: transparent url('../images/common_menu.png') repeat-x left top;
    border-radius: 5px;
    line-height: 37px;
    color:#333333;
    font-size:11px;
}

/* end header */

/* container sadrzi search meni, levi meni sa proizvodima, content i desni meni (korpa , napomena, najrpdavanjije ...) */
.container {
    width: 1100px;
    padding-left: 20px;
    padding-right: 20px;
    margin:0px auto;
    height:100%;

}
.content {
    width:726px;
    height:100%;
    float:left;
    padding:10px;
    padding-left:0;
}
/* end container */

/* search meni */
.G4 {
    height: 37px;
    margin-top:10px;
    color:#ffffff;
    font-size:11px;
    font-weight:600;
}

.G4_L input {
    color:#4C4C4C;
}

.G4_L input.selected {
    color:#e26f16;
}

.G4_L {
    height: 35px;
    float: left;
    border-radius: 5px 0px 0px 5px;
    border: solid 1px #b3b3b3;
    border-right:0;
    line-height: 35px;
    padding: 0px 15px 0px 15px;
    vertical-align:middle;
}

.G4_D {
    background: transparent url('../images/common_menu.png') repeat-x left top;
    border-radius: 5px;
    text-align: right;
    line-height: 37px;
}
/* end search meni */

/* meni proizvodi */
.side_menu {
    color: #666666;
    border: solid 1px #b3b3b3;
    border-radius: 5px 5px 0 0;
    background:#ffffff;
    margin-top:10px;
}

.side_menu ul li {
    border-top: 1px solid #e7e7e7;
}

.side_menu ul li a {
    font-size: 12px;
    font-weight:bold;
    color: #666666;
    display: block;
    padding: 6px 25px 6px 5px;
    background: transparent url('../images/side_menu_bullet.png') no-repeat 185px center;
}

.side_menu ul li a:hover {
    text-decoration: none;
    color: #e26f16;
}

.side_menu ul li.opened a {
    color: #e26f16;
}

.side_menu ul li.opened ul li a:hover {
    text-decoration: none;
    color: #e26f16;
}

.side_menu ul li.opened a {
    background: transparent url('../images/side_menu_bullet.png') no-repeat -397px center;
}

.side_menu ul li ul li.opened a {
    background: transparent url('../images/bullet.png') no-repeat 10px 9px;
    color: #e26f16;
}

.side_menu ul li ul li ul li.opened a {
    background: transparent url('../images/bullet.png') no-repeat 20px 9px;
    color: #e26f16;
}

.side_menu ul li.opened ul li a {
    background: transparent url('../images/bullet.png') no-repeat 10px 9px;
    color: #666666;
}

.side_menu ul li ul li.opened ul li a {
    background: transparent url('../images/bullet.png') no-repeat 20px 9px;
    color: #666666;
}

.side_menu ul li.opened ul li ul li a {
    background: transparent url('../images/bullet.png') no-repeat 20px 9px;
    color: #666666;
}

.side_menu ul li ul {
    border-top: 1px solid #e7e7e7;
    padding: 0;
    margin: 0;
}

.side_menu ul li ul li {
    border-top: none;
    background: #f8f8f8
}

.side_menu ul li ul li a {
    font-size: 12px;
    font-weight:bold;
    padding: 4px 5px 5px 15px;
    background: transparent url('../images/bullet.png') no-repeat 10px 9px;
}

.side_menu ul li ul li ul {
    border-top: none;
    padding: 0;
    margin: 0;
}

.side_menu ul li ul li ul li {
    border-top: none;
    background-color: white;
}

.side_menu ul li ul li ul li a {
    font-size: 11px;
    font-weight:bold;
    padding: 4px 5px 5px 25px;
    background: transparent url('../images/bullet.png') no-repeat 20px 9px;
    width: 164px;
}

.side_menu h3 {
    line-height:21px;
    font-size: 15px;
    font-weight:600;
    padding: 7px;
    margin:0px;
    color: #666666;
    border-radius: 5px 5px 0px 0px;

}
/* end meni proizvodi */


/* desni meni (korpa , napomena, najrpdavanjije ...) */
.D1 {
    text-align: center;
    /*color: gray;*/
    border-radius: 5px;
    padding-bottom:10px;
    margin-top:10px;
}

.D1 h3 {
    line-height:23px;
    padding:7px;
    border-radius: 5px 5px 0px 0px;
    font-size:11px;
}

.D1 h4 {
    color:#f2af26;
    margin: 10px;
    font-size:11px;
}

.D1 h5  {
    margin: 10px;
    font-size:10px;
}

.D1.najprodavanije {
    border: 1px solid #b3b3b3;
    padding-bottom:10px;
}
.D1.najprodavanije h3 {
    border-bottom: 1px solid #b3b3b3;
    margin-bottom: 10px;
    line-height: 19px;
    color: #4c4b4b;
}
.D1.najprodavanije .cena {
    height:10px;
    line-height:10px;
    font-size:10px;
    font-weight:600;
    color: #e26f16;
