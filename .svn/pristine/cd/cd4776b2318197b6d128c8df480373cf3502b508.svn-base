/* korpa */
table.korpa  {
    float: left;
    width:716px;
    line-height:22px;
    font-weight:600;
    text-align:center;
    margin-left:10px;
    margin-bottom:10px;
    color: #4c4b4b;
}
/*
table.korpa tr td img:hover {

}*/
table.korpa tr {
    vertical-align:top;
    height:30px;
}
table.korpa tr:hover {
    cursor:pointer;
    color:#e26f16;
}

table.korpa tr td {
    height:24px;
    line-height:12px;
}

table.korpa tr td > div {
    height:24px;
    line-height:12px;
    /*width:280px;*/
    text-align:left;
}

table.korpa th {
    height:24px;
    line-height:24px;
    font-size:12px;
    margin-bottom:5px;
}

.korpa_total {
    width:654px;
    border:1px solid #b3b3b3;
    background:#f2f2f2;
    font-size:11px;
    font-weight:600;
    padding:0px 30px;
    float:left;
    border-radius:5px;
    margin-left:10px;
    margin-bottom:10px;
    margin-top: 60px;
    color: #4c4b4b;
}

.korpa_total hr {
    margin:5px 20px 0px 20px;
}

.korpa_total imput {
    line-height:20px;
    padding:0;
    margin:0;
}
.korpa_total label {
    line-height:20px;

}

.korpa_total .prevoz_placanje {
    padding:0;
    width:148px;
    float:left;
    margin-top:20px;
    line-height:15px;

}
.korpa_total .prevoz_placanje label {
    padding:0;
    margin:0;
    margin-left:4px;
}
.korpa_total .total {
    width:354px;
    float:left;
    line-height:20px;
    text-align:right;
    margin-bottom:10px;
    margin-top:10px;

}

.korpa_total .total h1, h2 {
    font-size:14px;
}
.korpa_total .total h2 {
    color:#f2af26;
}

