	<subDataset name="generalinformation">
		<parameter name="ojP" class="java.lang.String"/>
		<queryString>
			<![CDATA[Select  Vrednost VrednostParametra
from     Sys_Vrednosti_Par sv, Sys_Parametri sp
where  sv.Organizaciona_Jedinica# = PKGB_OrgJed.Get_RootOj($P{ojP})
and     sv.Skraceni_Naziv = sp.Skraceni_Naziv
and     sp.Skraceni_Naziv_Zavisi_OD = 'PARSYS'
order  by sv.Skraceni_Naziv]]>
		</queryString>
		<field name="VREDNOSTPARAMETRA" class="java.lang.String"/>
	</subDataset>
	<subDataset name="porezsumarno">
		<parameter name="ojIN" class="java.lang.String">
			<parameterDescription><![CDATA[]]></parameterDescription>
			<defaultValueExpression><![CDATA["19"]]></defaultValueExpression>
		</parameter>
		<parameter name="godinaIN" class="java.lang.String">
			<defaultValueExpression><![CDATA["2010"]]></defaultValueExpression>
		</parameter>
		<parameter name="iddokumentaIN" class="java.lang.String">
			<parameterDescription><![CDATA[]]></parameterDescription>
			<defaultValueExpression><![CDATA["51"]]></defaultValueExpression>
		</parameter>
		<parameter name="idvdIN" class="java.lang.String">
			<defaultValueExpression><![CDATA["600"]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[select decode(vs.vrsta_stavke# , 20,'UKUPNA VRIJEDNOST:', 6,  'PDV ZA STOPU 17%:',
2, 'VRIJEDNOST BEZ PDV:', 3, 'IZNOS RABATA:',  8,  'OSNOVICA ZA STOPU 17%:', vs.opis||':') opis,
 decode( pvd.ino_dom, 'D', VREDNOST, DEVIZNA_VREDNOST) vrednost_suma
 from prod_STAVKA_FIN_DOKUMENTA   sfd, prod_vrsta_dokumenta pvd, ocp_vrsta_stavke vs
 where  to_char(sfd.id_vd) = to_char('$P!{idvdIN}')
and to_char(sfd.organizaciona_jedinica#) = to_char('$P!{ojIN}')
and to_char(sfd.godina) = to_char('$P!{godinaIN}')
and to_char(sfd.id_fin_dokumenta) = to_char('$P!{iddokumentaIN}')
 and  sfd.id_vd = pvd.id_vd
and vs.vrsta_stavke# = sfd.vrsta_stavke#
            and sfd.vrsta_stavke# in (select v.vrsta_stavke#
                                                 from ocp_vrsta_stavke v
                                                where v.tip in ('S', 'A','W', 'R', 'P') and vrsta_stavke# <> 108
                                                )
order by vs.stampa_se]]>
		</queryString>
		<field name="OPIS" class="java.lang.String"/>
		<field name="VREDNOST_SUMA" class="java.math.BigDecimal"/>
	</subDataset>
	<subDataset name="ponalogu">
		<parameter name="idvdINN" class="java.lang.String">
			<parameterDescription><![CDATA[]]></parameterDescription>
		</parameter>
		<parameter name="iddokumentaINN" class="java.lang.String"/>
		<parameter name="ojINN" class="java.lang.String"/>
		<parameter name="godinaINN" class="java.lang.String"/>
		<queryString language="SQL">
			<![CDATA[select * from prod_fakturisan_dokument
where to_char(id_vd_f) = to_char('$P!{idvdINN}')
and to_char(organizaciona_jedinica#) = to_char('$P!{ojINN}')
and to_char(godina) = to_char('$P!{godinaINN}')
and to_char(id_fin_dokumenta) = to_char('$P!{iddokumentaINN}')]]>
		</queryString>
		<field name="ID_DOKUMENTA" class="java.lang.String"/>
	</subDataset>
	<subDataset name="WoUser">
		<parameter name="idvdINNN" class="java.lang.String">
			<parameterDescription><![CDATA[]]></parameterDescription>
		</parameter>
		<parameter name="iddokumentaINNN" class="java.lang.String"/>
		<parameter name="ojINNN" class="java.lang.String"/>
		<parameter name="godinaINNN" class="java.lang.String"/>
		<queryString>
			<![CDATA[SELECT DISTINCT
     Initcap(w.ime||' '||w.prezime) as neko,
     max(to_char(r.datumivreme, 'dd.mm.rrrr, hh24:mi:ss')) as datum
FROM
     "PROD_FAKTURISAN_DOKUMENT" f INNER JOIN "UZ_DOKUMENT" u ON f."ID_VD" = u."ID_VD"
     AND f."ID_DOKUMENTA" = u."ID_DOKUMENTA"
     INNER JOIN "WO_REZERVACIJA" r ON u."SPOLJNI_BROJ_DOKUMENTA" = r."SESSIONID"
     INNER JOIN "WO_USER" w ON r."USER_ID" = w."ID"
WHERE to_char(f.id_vd_f) = to_char($P{idvdINNN})
and to_char(f.organizaciona_jedinica#) = to_char($P{ojINNN})
and to_char(f.godina) = to_char($P{godinaINNN})
and to_char(f.id_fin_dokumenta) = to_char($P{iddokumentaINNN})
group by Initcap(w.ime||' '||w.prezime)]]>
		</queryString>
		<field name="NEKO" class="java.lang.String"/>
		<field name="DATUM" class="java.lang.String"/>
	</subDataset>
	<parameter name="idvd" class="java.lang.String">
		<defaultValueExpression><![CDATA["600"]]></defaultValueExpression>
	</parameter>
	<parameter name="oj" class="java.lang.String">
		<defaultValueExpression><![CDATA["30"]]></defaultValueExpression>
	</parameter>
	<parameter name="godina" class="java.lang.String">
		<defaultValueExpression><![CDATA["2013"]]></defaultValueExpression>
	</parameter>
	<parameter name="iddokumenta" class="java.lang.String">
		<defaultValueExpression><![CDATA["7"]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT od.naziv_vd, parm.naziv, parm.pib,parm.adresa, FD.ID_VD||'-'||FD.ID_FIN_DOKUMENTA iddokumenta, FD.ID_VD, FD.ORGANIZACIONA_JEDINICA#, FD.GODINA, FD.ID_FIN_DOKUMENTA, FD.ID_FIN_DOKUMENTA_PO, FD.BROJ_POSLEDNJE_STAVKE,
FD.STATUS_DOK, FD.DATUM_FORMIRANJA, nvl(FD.DATUM_DPO,FD.DATUM_FORMIRANJA) datum_dpo,FD.RADNIK#, FD.POSLOVNI_PARTNER#_PL, FD.POSLOVNI_PARTNER#_PR, FD.BROJ_IZJAVE, FD.DATUM,
FD.DATUM_IZJAVE, FD.ID_CENOVNIK_PP, FD.NACIN_OTPREME,  FD.NAPOMENA, FD.PROC_KASSA_SKONTO,FD.PROC_NATURALNI_RABAT, FD.RADNIK#_TP, id_valute,
decode(datum_kursne_liste#, null, datum_formiranja, datum_kursne_liste#) datum_kursne_liste#, decode(vrsta_kursne_liste#,null, 1, vrsta_kursne_liste#) vrsta_kursne_liste#,
FD.VREDNOST_ZA_UPLATU, ugovor_broj, pvd.clan_zakona, pvd.pdv, ino_dom, vrsta_fin_dok,datum_isporuke, p.naziv_proizvoda||' '||p.dodatni_naziv nazivproizvoda, fds.ID_JEDINICE_MERE,
fds.ID_VD STAVKA_ID_VD, fds.JEDINICNA_CENA, fds.KOLICINA, fds.akciza, fds.ORGANIZACIONA_JEDINICA# STAVKA_OJ, fds.PROCENT_RABATA, fds.PROIZVOD#, fds.RB_STAVKE_FINDOK, fds.VREDNOST,
fds.iznos_poreza,fds.procenat_poreza,fds.VREDNOST_RABATA,fds.vrsta_stavke# ,round(fds.otpremljena_vrednost,2) otp_vred,
(round(fds.otpremljena_vrednost,2)+round(nvl(fds.akciza,0),2)) vred_sa_akc, (round(otpremljena_vrednost,2)+round(nvl(fds.akciza,0),2))/kolicina cena_sa_akc, jm.skracena_oznaka,
(round(fds.vrednost,2)+round(nvl(fds.iznos_poreza,0),2)) vrednostzauplatustavka,
PRIKAZ_SLOVIMA (FD.VREDNOST_ZA_UPLATU, id_valute) vrednosslovima, w.logo_path,
round((jedinicna_cena+(jedinicna_cena*procenat_poreza/100))*(1-(procent_rabata/100)),2) cenasaporezom,
fd.nacin_placanja, pl.naziv vrstaplacanja
FROM ocp_vrsta_dokumenta od, prod_vrsta_dokumenta pvd, prod_FIN_DOKUMENT FD, prod_stavka_fin_dokumenta fds, ocp_proizvod p,
     ocp_jedinica_mere jm, (select par.pib, par.poslovni_partner#, par.naziv, par.adresa||' '||m.naziv||' '||m.ptt_broj adresa
							from ocp_poslovni_partner par, ocp_mesto m
							where par.id_mesta = m.id_mesta) parm, wo_kompanija_korisnik w, prod_nacin_placanja pl
where to_char(od.id_vd) = to_char('$P!{idvd}')
and pvd.id_vd = od.id_vd
and pvd.id_vd = fd.id_vd
and to_char(fd.organizaciona_jedinica#) = to_char('$P!{oj}')
and to_char(fd.godina) = to_char('$P!{godina}')
and to_char(fd.id_fin_dokumenta) = to_char('$P!{iddokumenta}')
and fd.poslovni_partner# = parm.poslovni_partner#
and fd.id_vd = fds.id_vd
and fd.organizaciona_jedinica# = fds.organizaciona_jedinica#
and fd.godina = fds.godina
and fd.id_fin_dokumenta = fds.id_fin_dokumenta
and fds.proizvod# = p.proizvod#
and p.jedinica_mere# = jm.id_jedinice_mere
and w.corresponding_oj = PKGB_OrgJed.Get_RootOj(fd.organizaciona_jedinica#)
and pl.nacin# = fd.nacin_placanja
order by  fds.RB_STAVKE_FINDOK]]>
	</queryString>
